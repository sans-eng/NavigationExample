<Page x:Class="NavigationExample.Views.DataPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:NavigationExample.Views" 
      xmlns:viewmodels="clr-namespace:NavigationExample.ViewModels" 
      d:DataContext="{d:DesignInstance Type=viewmodels:DataViewModel}"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="DataView">

    <Grid>
        <TextBlock HorizontalAlignment="Center"
                   VerticalAlignment="Center"
                   FontSize="30" Text="{Binding Data}"/>
    </Grid>
</Page>
